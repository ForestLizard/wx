/* pages/p01-03/p01-03.wxss */
.nav{
  display: flex;
  justify-content: center;
  position: fixed;
  width: 100%;
  height: 86rpx;
  background-color: #fff;
}
.nav-item{
  width: 148rpx;
  height: 86rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #999;
  font-size: 15px;
}
.nav-item-now{
  position: relative;
  color: #56c0b0;

}
.nav-item-now ::after{
  content: "";
  position: absolute;
  width: 100%;
  left:0;
  height: 2rpx;
  bottom: 0;
  background-color: #56c0b0;
}

.cont{
  padding-top: 86rpx;
  box-sizing: border-box;
}


.box{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 1206rpx;
}
.box image{
  width: 200rpx;
  height: 200rpx;
  margin-top: 100rpx;
  margin-bottom: 30rpx;
}
.tip1, .tip2{
  color: #999;
  font-size: 14px;
}
.tip1{
  padding-bottom: 20rpx;
}
button{
  height: 68rpx !important;
  line-height: 68rpx !important;
  margin-top: 40rpx !important;
  padding: 0 104rpx !important;
  font-size: 14px!important;
  border-radius: 34rpx!important;
}