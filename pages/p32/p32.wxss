/* pages/p32/p32.wxss */
.nav-bar-black{
   justify-content: flex-end; 
}
.nav-bar-black image{
  height: 40rpx;
  width: 40rpx;
  margin-right: 30rpx;
 
}

.header{
  position: fixed;
  top:86rpx;
  left: 0;
  display: flex;
  background-color: #f9f9f9;
  padding: 30rpx 0;

}
.header-item{
  width: 185rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-right: 2rpx solid #eee;
  color: #666;
  font-size: 12px;
}
.header-item-last{
  border: 0;
}
.header-item image{
  width: 30rpx;
  height: 30rpx;
}

.cont{
  padding-top: 170rpx;
}

.salary-box{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 750rpx;
  transition: 0.6s 0s;
  z-index: 12
}

.sec {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #eee;
  color: #333;
  background-color: #fff;
  font-size: 14px; 
}
.sec text{
  padding-left: 30rpx;
}


.sec-first {
  justify-content: center;
  position: relative;
}
.sec-first image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}


