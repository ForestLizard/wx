/* pages/p92/p92.wxss */

.content,.footer{
  background-color: #ffffff;
}
.placeholder{
  padding-left: 12rpx;
  color: #bbb;
  font-family: '微软雅黑';
  font-size:12px;
  
}

.header{
  height: 84rpx;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
  font-size: 12px;
  color:#999;
}


/*content*/
.register{
  height: 1206rpx;
 
}
.register input{
  margin-left: 30rpx;
  font-size: 12px;
  color: #bbb
}
.content{
  position: relative;
  height: 174rpx;
  background-color: #fff;
  border-bottom: 20rpx solid #eeeeee;
}
.content .tel{
  display: block;
  height: 87rpx;
  border-bottom: 1rpx solid #eeeeee;

}
.content .verify{
  display: block;
  height: 86rpx;
}

.primary{
  width: 156rpx;
  height: 44rpx;
  margin: 0;
  padding: 0;
  box-sizing: content-box;
  line-height: 44rpx;
  border: 1rpx solid #56c0b0;
  color: #56c0b0;
  border-radius: 22rpx;
  font-size: 10px;
  overflow: visible;
}

.inverse{
  color: #ffffff;
  background-color: #56c0b0
}
.top{
  position: absolute;
  top: 45rpx;
  right: 30rpx;
}
.bottom{
  position: absolute;
  bottom: 20rpx;
  right: 30rpx;
}
/*content end*/


/*footer*/

.footer{
  margin-top: 44rpx;
  height: 67rpx;
  padding: 0 30rpx 712rpx 30rpx;
  background-color: #f0f0f0
}
.full{
  width: 690rpx;
  height: 67rpx;
  line-height: 67rpx;
  border-radius: 6rpx;
  font-size: 14px;
}

/*footer end*/
