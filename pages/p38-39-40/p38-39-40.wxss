/* pages/p38-39-40/p38-39-40.wxss */
.nav-bar{
  border-bottom: 1rpx solid #d2d2d2;
  display: flex;
  align-items: center;
  width: 750rpx;
}
.nav-bar image{
  width: 40rpx;
  height: 40rpx;
  padding-left: 30rpx;
}
.cont{
  padding-top: 87rpx;
  padding-bottom: 74rpx;
  background-color: #fff;
}
.group{
  padding-bottom: 0;
  display: flex;
  flex-direction: column;
}
.group-header{
  display: flex;
  align-items: center;
  color: #999;
  font-size: 14px;
  padding-bottom: 20rpx;
}
.group-header image{
  width: 30rpx;
  height: 30rpx;
  padding-right: 20rpx;
}

.group-content{
  display: flex;
  flex-wrap: wrap;
}
.bt-primary, .bt-reject{
  margin-right: 20rpx;
  margin-bottom: 30rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.bt-reject{
  color: #666;
  background-color: #eee;
}

.group-header-2{
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #999;
  font-size: 14px;
  padding-bottom: 20rpx;
}
.group-left{
  display: flex;
  align-items: center;
}
.group-left image{
  width: 30rpx;
  height: 30rpx;
  padding-right: 20rpx;
}
.group-right{
  width: 30rpx;
  height: 30rpx;
  padding-right: 20rpx;
}

.footer{
  position: fixed;
  bottom: 0;
  display: flex;
}
#reset{
  width: 375rpx;
  height: 74rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #999;
  font-size: 16px;
  background-color: #f0f0f0;

}
#comfirm{
  width: 375rpx;
  height: 74rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 16px;
  background-color: #56c0b0;

}