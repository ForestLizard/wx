/* pages/cicle/cicle.wxss */

.placeholder{
  font-family: '微软雅黑';
  color: #999999;
  text-align: right;
}
.placeholder-left{
  font-family: '微软雅黑';
  color: #eeeeee;
}
.content{
  background-color: #ffffff
}
input{
  min-height: 0;
}
.form-header{
  border-bottom: 15rpx solid #eeeeee;
}

.form-group{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 14px;
}
.form-right{
  display: flex;
  align-items: center;
}
.avatar{
  width: 80rpx;
  height: 80rpx;

}
.go{
  width: 30rpx;
  height: 30rpx;
  padding-left: 10rpx;
}
.picker{
  color:#999999;
}


.form-sec-vertical{
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: space-between;
  margin-left: 30rpx;
  padding:30rpx 30rpx 30rpx 0;
  border-bottom: 1rpx solid #eeeeee;
  font-size: 14px;
}
#match-label{
  height: 160rpx;
}
.match-label{
  display: flex;
  align-items: center;
}
.bt{
  display:flex;
  align-items: center;
  font-size:10px;
  color: #56c0b0;
  border-radius: 23rpx;
  height: 46rpx;
  margin: 5rpx 20rpx 5rpx 0;
  padding: 0 23rpx;
  border:1rpx solid #56c0b0
}

.bt image{
  position: absolute;
  top: -15prx;
  right: 0;
}
.bt-able{
  position: relative;
  overflow: visible;
}
.bt-able::after{
  content: url('data:image/png;base64,');
  position: absolute; 
  top: -15rpx !important;
  left:auto;
  right: 20rpx !important;
  width: 0rpx;
  height: 0rpx;
  z-index: 100;
}
.bt-inverse{
  color:#ffffff;
  background-color: #56c0b0;
}
.bt-large{
  font-size: 14px;
}
#add{
  position: absolute;
  margin: 0rpx;
  bottom: 5rpx;
  right: 30rpx;
}

#recommand-label{
  min-height: 130rpx;
  padding: 30rpx;
  margin: 0;
  justify-content: flex-start;
}
.recommand-label{
  display: flex;
  align-items: center
}
#change{
  font-size: 14px;
  padding-left: 40rpx;
}

.footer text{
  padding: 30rpx;
  color:#999999;
  font-size: 10px;
}